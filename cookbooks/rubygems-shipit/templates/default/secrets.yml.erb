production:
  secret_key_base: <%= @secret_key_base %>
  redis_url: "<%= @redis_url %>"
  github_api:
    access_token: <%= @github_credentials['access_token'] %>
  github_oauth:
    id: "<%= @auth_keys['github_client_id'] %>"
    secret: "<%= @auth_keys['github_client_secret'] %>"
    team: 'rubygems/deployers'
  host: '<%= @hostname %>'
  env:
    HOME: '<%= @home_dir %>/'
